<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.edu.hznu.moneykeeper">

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Base"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddcostActivity"
            android:label="@string/add_activity_name"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AddcostActivity"> <!-- 返回上一个活动 -->

        </activity>
        <activity
            android:name=".EditAddActivity"
            android:label="@string/add_activity_name"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/EditAddActivity"> <!-- 返回上一个活动 -->

        </activity>
        <activity
            android:name=".LookBackActivity"
            android:label="@string/lookback"
            android:parentActivityName=".ChartActivity"
            android:screenOrientation="portrait"
            android:theme="@style/LookBackActivity"></activity>

        <activity android:name=".ChartActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ChartActivity">

        </activity>
    </application>

</manifest>